{
  "name": "dotenv-generator",
  "version": "0.1.7",
  "description": "Creates an env file from a template or an example .env file",
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "type": "module",
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  },
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "start": "./lib/index.js",
    "test": "npx jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "coverage": "npx jest --coverage"
  },
  "files": [
    "lib/generator.js",
    "lib/parser.js",
    "lib/validators.js",
    "lib/index.js"
  ],
  "keywords": [
    "env",
    "dotenv",
    "generator",
    "environment",
    "setup",
    "wizard"
  ],
  "author": "https://github.com/lnenad",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.0.0",
    "dotenv": "^16.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "inquirer": "^9.2.8"
  },
  "devDependencies": {
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^27.5.1",
    "prettier": "^3.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
